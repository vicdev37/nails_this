    "parcel-bundler": "^1.12.3",
    "pug": "^2.0.3",
    "push-dir": "^0.4.1",
    "sass": "^1.26.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "autoprefixer": "^9.5.1",
